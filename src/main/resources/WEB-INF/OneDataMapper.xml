<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.doubledb.Config.DataSourceDAO.OneData">
    <select id="selectSalary" resultType="com.doubledb.model.SalaryModel" parameterType="Long">
        select `one-data`.salary.ID, NAME, EMAIL from salary WHERE id = #{value}
    </select>
</mapper>